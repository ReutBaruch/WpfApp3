<Window x:Class="WpfApp3.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp3"
        xmlns:Views ="clr-namespace:WpfApp3.Views"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="800">
    <Window.Resources>
        <Style x:Key="settings">
            <Setter Property="Button.Background" Value="PapayaWhip"/>
            <Setter Property="Button.Height" Value="Auto"/>
            <Setter Property="Button.Width" Value="Auto"/>
        </Style>
    </Window.Resources>
    <Grid>
        <!--Views:Manual-->
        <Views:FlightBoard/>
        <!--Views:AutoPilot Loaded="AutoPilot_Loaded"-->
    </Grid>
</Window>
